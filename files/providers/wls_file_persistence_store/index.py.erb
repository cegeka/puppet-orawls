connect('<%= weblogicUser %>','<%= weblogicPassword %>'  ,'<%= weblogicConnectUrl %>')

def quote(text):
    if text:
        return "\"" + str(text).replace("\"", "\"\"") + "\""
    else: 
        return ""

m = ls('/FileStores')

f = open("/tmp/wlstScript.out", "w")
print >>f, "name;directory;target;targettype"
for token in m.split("dr--"):
    token=token.strip().lstrip().rstrip()
    if token:
        print '___'+token+'___'
        cd('/FileStores/'+token)

        directory = get('Directory')

        n = ls('/FileStores/'+token+'/Targets')
        target = ''

        for token2 in n.split("dr--"):
            token2=token2.strip().lstrip().rstrip()
            if not token2 == '':
               target = token2
               cd('/FileStores/'+token+'/Targets/'+token2)
               targetType = get('Type')

        print >>f, ";".join(map(quote, [token,directory,target,targetType]))
f.close()

disconnect()
exit()
